{% extends 'base.html.twig' %}

{% block title %}Nouvelle réservation{% endblock %}

{% block body %}
<div class="container mt-4">
    <h1 class="mb-4">Nouvelle réservation</h1>
    {{ form_start(bookingForm) }}
        {{ form_row(bookingForm.computer) }}
        {{ form_row(bookingForm.startTime) }}
        {{ form_row(bookingForm.endTime) }}
        {{ form_row(bookingForm.forfait) }}
        <button type="submit" class="btn btn-success">Réserver</button>
    {{ form_end(bookingForm) }}
</div>
{% endblock %}